package kb04.ditto.jgig.entity;

import java.util.List;

public class Paging {
	
	private int total; //전체 글 개수
	private int currentPage; //현재페이지 번호
	private int totalPages; //전체 페이지 개수
	private int startPage; //첫 페이지
	private int endPage; //끝 페이지
	private int pagingNum; //페이징 개수
	private List<BoardDto> content; //글 데이터
	
	//생성자
	public Paging(int total, int currentPage, int totalPages, int startPage, int endPage, int pagingNum,
			List<BoardDto> content) {
		super();
		this.total = total;
		this.currentPage = currentPage;
		this.totalPages = totalPages;
		this.startPage = startPage;
		this.endPage = endPage;
		this.pagingNum = pagingNum;
		this.content = content;
	}
	
	

}
