package kb04.ditto.jgig.entity;

public class SuggestionDto {
	String sgn_id;
	int brd_id;
	
	public SuggestionDto() {
		// TODO Auto-generated constructor stub
	}

	public SuggestionDto(String sgn_id, int brd_id) {
		super();
		this.sgn_id = sgn_id;
		this.brd_id = brd_id;
	}

	public String getSgn_id() {
		return sgn_id;
	}

	public void setSgn_id(String sgn_id) {
		this.sgn_id = sgn_id;
	}

	public int getBrd_id() {
		return brd_id;
	}

	public void setBrd_id(int brd_id) {
		this.brd_id = brd_id;
	}

	@Override
	public String toString() {
		return "sgn_id=" + sgn_id + ", brd_id=" + brd_id;
	}
}
