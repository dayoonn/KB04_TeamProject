package kb04.ditto.jgig.entity;

public class CommentDto {
	int cmt_id, brd_id, mem_id, cmt_declaration, cmt_suggestion;
	String regdate, cmt_content;
	
	public CommentDto() {
		// TODO Auto-generated constructor stub
	}

	public CommentDto(int cmt_id, int brd_id, int mem_id, int cmt_declaration, int cmt_suggestion, String regdate,
			String cmt_content) {
		super();
		this.cmt_id = cmt_id;
		this.brd_id = brd_id;
		this.mem_id = mem_id;
		this.cmt_declaration = cmt_declaration;
		this.cmt_suggestion = cmt_suggestion;
		this.regdate = regdate;
		this.cmt_content = cmt_content;
	}

	public int getCmt_id() {
		return cmt_id;
	}

	public void setCmt_id(int cmt_id) {
		this.cmt_id = cmt_id;
	}

	public int getBrd_id() {
		return brd_id;
	}

	public void setBrd_id(int brd_id) {
		this.brd_id = brd_id;
	}

	public int getMem_id() {
		return mem_id;
	}

	public void setMem_id(int mem_id) {
		this.mem_id = mem_id;
	}

	public int getCmt_declaration() {
		return cmt_declaration;
	}

	public void setCmt_declaration(int cmt_declaration) {
		this.cmt_declaration = cmt_declaration;
	}

	public int getCmt_suggestion() {
		return cmt_suggestion;
	}

	public void setCmt_suggestion(int cmt_suggestion) {
		this.cmt_suggestion = cmt_suggestion;
	}

	public String getRegdate() {
		return regdate;
	}

	public void setRegdate(String regdate) {
		this.regdate = regdate;
	}

	public String getCmt_content() {
		return cmt_content;
	}

	public void setCmt_content(String cmt_content) {
		this.cmt_content = cmt_content;
	}

	@Override
	public String toString() {
		return "cmt_id=" + cmt_id + ", brd_id=" + brd_id + ", mem_id=" + mem_id + ", cmt_declaration="
				+ cmt_declaration + ", cmt_suggestion=" + cmt_suggestion + ", regdate=" + regdate + ", cmt_content="
				+ cmt_content;
	}
	
	

}
